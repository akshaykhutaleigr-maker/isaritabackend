model SubSubMenu {
  id          BigInt      @id @default(autoincrement())
  sub_menu_id BigInt?
  name_en     String?     @db.VarChar(255)
  controller  String?     @db.VarChar(255)
  action      String?     @db.VarChar(255)
  created     DateTime?
  modified    DateTime?
  user_id     Int?
  name_ll     String?     @db.VarChar(255)
  name_ll1    String?     @db.VarChar(255)
  name_ll2    String?     @db.VarChar(255)
  name_ll3    String?     @db.VarChar(255)
  name_ll4    String?     @db.VarChar(255)
  display_order Decimal?
  req_ip      String?     @db.VarChar(100)
  state_id    Decimal?
  updated     DateTime?
  dv_remarks  String?     @db.VarChar(1000)

  submenu     SubMenu?    @relation(fields: [sub_menu_id], references: [id])
  permissions UserPermissions[] @relation("SubSubmenuPermissions")

  @@map("ngdrstab_mst_subsubmenu")
}
