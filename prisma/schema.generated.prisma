  state_id            Int?
  division_id         Int?
  district_id         Int?
  taluka_id           Int?
  email_id            String?  @db.VarChar(100) @unique
  mobile_no           String?  @db.VarChar(12) @unique
  id_type             Int?
  pan_no              String?  @db.VarChar(255)
  user_name           String?  @db.VarChar(50) @unique
  user_pass           String?  @db.VarChar(250)
  hint_question       Int?
