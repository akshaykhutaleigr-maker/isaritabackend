  regfor              String?  @db.VarChar(25)
  hod_name            String?  @db.VarChar(250)
  hod_email           String?  @db.VarChar(250)
  hod_phone           String?  @db.VarChar(250)
  hod_signedfile      String?  @db.VarChar(250)
  hash_code           String?  @db.VarChar(255)
  hash_code_flag      String?  @default("N") @db.Char(1)
  uploaded_sign_file  String?  @db.VarChar(250)
  fileupload_ip       String?
