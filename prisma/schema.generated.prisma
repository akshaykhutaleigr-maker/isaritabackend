datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}




model CitizenRegistration {
  reg_id              BigInt   @id @default(autoincrement())
  contact_fname       String?  @db.VarChar(100)
